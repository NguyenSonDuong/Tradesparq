{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EfCore": {
    "AutoMigrate": true, // bật/tắt auto migrate
    "EnableSensitiveDataLogging": false
  },
  "Jobs": {
    "MyJob": {
      "IntervalSeconds": 3600, // lặp lại mỗi 60s
      "RunOnStart": true // chạy ngay khi app khởi động
    }
  }
}
